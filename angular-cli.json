{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "rent-clothes-app"
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
            "styles.css",
            "../node_modules/datatables.net-dt/css/jquery.dataTables.css"
        ],
        "scripts": [
            "../node_modules/jquery/dist/jquery.min.js",
            "../node_modules/bootstrap/dist/js/bootstrap.js",
            "../node_modules/arrive/src/arrive.js",
            "../node_modules/moment/moment.js",
            "../node_modules/chartist/dist/chartist.js",
            "../node_modules/bootstrap-notify/bootstrap-notify.js",
            "../node_modules/datatables.net/js/jquery.dataTables.js",
            "../src/assets/seiko/js/vendor/swiper/swiper.min.js",
            "../src/assets/seiko/js/vendor/slick/slick.min.js",
            "../src/assets/seiko/js/vendor/parallax/parallax.js",
            "../src/assets/seiko/js/vendor/isotope/isotope.pkgd.min.js",
            "../src/assets/seiko/js/vendor/magnificpopup/dist/jquery.magnific-popup.js",
            "../src/assets/seiko/js/vendor/nouislider/nouislider.min.js",
            "../src/assets/seiko/js/vendor/tocca/tocca.min.js",
            "../src/assets/seiko/js/vendor/bootstrap-tabcollapse/bootstrap-tabcollapse.js",
            "../src/assets/seiko/js/vendor/scrollLock/jquery-scrollLock.min.js",
            "../src/assets/seiko/js/vendor/darktooltip/dist/jquery.darktooltip.js",
            "../src/assets/seiko/js/vendor/imagesloaded/imagesloaded.pkgd.min.js",
            "../src/assets/seiko/js/megamenu.min.js",
            "../src/assets/seiko/js/tools.min.js",
            "../src/assets/seiko/js/app.js"
        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "lint": [{
            "project": "src/tsconfig.app.json"
        },
        {
            "project": "src/tsconfig.spec.json"
        },
        {
            "project": "e2e/tsconfig.e2e.json"
        }
    ],
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "serve": {
            "port": 4400
        },
        "component": {}
    }
}